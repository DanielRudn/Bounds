package com.dr.bounds.animations;

import java.util.ArrayList;

import com.DR.dLib.AnimationStatusListener;
import com.DR.dLib.dAnimation;
import com.DR.dLib.dObject;

public class SlideInArrayAnimation extends dAnimation {

	private ArrayList<dObject> animObjects;
	
	public SlideInArrayAnimation(ArrayList<dObject> objectsToAnimate, float duration, AnimationStatusListener listener, int ID) {
		super(null, duration, listener, ID);
		animObjects = objectsToAnimate;
	}

	@Override
	protected void animate(float time, float duration, float delta) {
		
	}

	@Override
	public void start()
	{
		super.start();
	}
}
